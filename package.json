{
	"name": "violet-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "storybook build -o build",
		"start": "npx http-server build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook-server": "start-storybook --loglevel verbose --no-version-check --quiet --ci --output-dir='./storybook-server' --quiet --nodeLogger --static-dir='./public' --config-dir='./.storybook' --https",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@storybook/addon-essentials": "^7.0.0-rc.10",
		"@storybook/addon-info": "^5.3.21",
		"@storybook/addon-interactions": "^7.0.0-rc.10",
		"@storybook/addon-links": "^7.0.0-rc.10",
		"@storybook/addon-mdx-gfm": "^7.0.0-rc.10",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^3.0.0-next.4",
		"@storybook/blocks": "^7.0.0-rc.10",
		"@storybook/builder-vite": "^7.0.0",
		"@storybook/node-logger": "^6.5.16",
		"@storybook/svelte": "^7.0.0-rc.10",
		"@storybook/sveltekit": "^7.0.0-rc.10",
		"@storybook/testing-library": "^0.0.14-next.1",
		"@storybook/types": "^7.0.0-rc.10",
		"@sveltejs/adapter-node": "^1.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"axios": "^1.3.4",
		"clsx": "^1.2.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-storybook": "^0.6.11",
		"eslint-plugin-svelte3": "^4.0.0",
		"felte": "^1.2.7",
		"http-server": "^14.1.1",
		"node-sass": "^7.0.1",
		"postcss": "^8.4.14",
		"postcss-cli": "^10.0.0",
		"postcss-import": "^14.1.0",
		"postcss-nesting": "^10.1.10",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.53.0",
		"storybook": "^7.0.0-rc.10",
		"storybook-addon-sass-postcss": "^0.1.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-jester": "^2.3.2",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"svelte-spa-router": "^3.3.0",
		"tailwindcss": "^3.2.7",
